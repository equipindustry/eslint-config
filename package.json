{
  "name": "@equipindustry/eslint-config",
  "description": "Shared ESLint config for Equipindustry projects",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+htps://github.com/equipindustry/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "lint",
    "formatting"
  ],
  "files": [
    ".eslintrc.js",
    "eslint"
  ],
  "main": ".eslintrc.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "prettier": "prettier '**/*.{js?(on),ts?(x),scss,md,yml}' --write=false --list-different",
    "prettier:fix": "yarn prettier --write=true"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@equipindustry/prettierrc": "^0.2.0",
    "husky": "^4.2.5",
    "semantic-release": "^17.0.7"
  },
  "dependencies": {
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ban": "^1.4.0",
    "eslint-plugin-etc": "^0.0.1-beta.25",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^24.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
